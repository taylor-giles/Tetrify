{
  "name": "tetrify",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "ui/index.cjs",
  "license": "MIT",
  "author": {
    "name": "Taylor Giles",
    "email": "contact@taylorgiles.me"
  },
  "keywords": [
    "tetrify",
    "tetromino",
    "tetrominoes",
    "tetris",
    "art"
  ],
  "description": "An app for building custom animations of falling Tetrominoes",
  "scripts": {
