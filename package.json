    "svelte-check": "^3.0.0",
    "svelte-preprocess": "^5.0.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.0"
  },
  "dependencies": {
    "@types/watch": "^1.0.3",
    "electron-squirrel-startup": "^1.0.0",
    "fs": "^0.0.1-security",
    "gif.js": "^0.2.0",
    "gif.js.optimized": "^1.0.1",
    "os": "^0.1.2",
    "sirv-cli": "^2.0.0"
